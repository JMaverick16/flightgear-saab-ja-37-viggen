    <animation>
        <type>select</type>
        <object-name>Pilot</object-name>
        <condition>
            <not>
                <equals>
                    <property>sim/current-view/internal</property>
                    <value>1</value>
                </equals>
            </not>
            <not>
                <equals>
                    <property>ja37/done</property>
                    <value>1</value>
                </equals>
            </not>
        </condition>
    </animation>
