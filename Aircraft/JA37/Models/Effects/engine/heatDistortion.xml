<?xml version="1.0"?>

<PropertyList>
  <nopreview/>

   <name>HeatDistortion</name>
   <path>heatDistortion.ac</path>

   <offsets> 
    <x-m>0</x-m>
    <y-m>0</y-m>
    <z-m>0</z-m>
    <heading-deg>0</heading-deg>
   </offsets>
   
  <effect>
   <inherits-from>heatDistortion</inherits-from>
   <object-name>Bounding_box</object-name>
  </effect>

  <animation>
    <type>select</type>
    <object-name>Bounding_box</object-name>
    <nopreview/>
    <condition>
     <property>sim/multiplay/generic/int[3]</property><!-- engines/engine[0]/running -->
     <not>
      <property>sim/multiplay/generic/int[8]</property><!-- engines/engine[0]/is-reversed -->
     </not>
    </condition>
  </animation>

  <animation>
    <type>scale</type>
    <object-name>Bounding_box</object-name>
    <nopreview/>
    <x-offset>2.5</x-offset>
    <y-offset>10</y-offset>
    <z-offset>10</z-offset>
  </animation>

    <!--<f>
        <radius type="double">0.0001</radius>
