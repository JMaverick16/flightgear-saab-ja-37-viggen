<?xml version="1.0"?>


<PropertyList>

  <path>Models/Effects/procedural_light.ac</path>

 <effect>
    <inherits-from>Aircraft/JA37/Models/Effects/proc-als-lights/landing-light</inherits-from>
   <object-name>procedural_light</object-name>
   <parameters>
        
    </parameters>
 </effect>
 
  <nopreview/>

  <animation>
    <type>noshadow</type>
    <object-name>procedural_light</object-name>
  </animation>

  <!-- A better solution would be to use
       <intensity_scale><use>property</use></intensity_scale>
       in the corresponding .eff file, but this does not work with MP properties
       because they are tied. As a replacement, this does a reasonable job.
  -->
  <animation>
    <type>scale</type>
    <object-name>procedural_light</object-name>
    <property>sim/multiplay/generic/float[4]</property> <!-- /ja37/effect/landing-light -->
  </animation>
</PropertyList>
