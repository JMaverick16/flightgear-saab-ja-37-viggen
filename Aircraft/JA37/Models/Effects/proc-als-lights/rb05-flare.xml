<?xml version="1.0"?>

<PropertyList>
  <path>Models/Effects/procedural_light.ac</path>
    
  <effect>
    <inherits-from>Aircraft/JA37/Models/Effect/proc-als-lights/rb05-flare</inherits-from>
    <object-name>procedural_light</object-name>
  </effect>

  <animation>
    <type>scale</type>
    <object-name>procedural_light</object-name>
    <x-offset>10</x-offset>
    <y-offset>10</y-offset>
    <z-offset>10</z-offset>
  </animation>

  <animation>
    <type>dist-scale</type>
    <object-name>procedural_light</object-name>
    <interpolation>
      <entry><ind>0</ind><dep>0.5</dep></entry>
      <entry><ind>6000</ind><dep>30</dep></entry>
    </interpolation>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>procedural_light</object-name>
  </animation>
</PropertyList>
