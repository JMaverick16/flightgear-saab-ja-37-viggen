        <object-name>switchR</object-name>
        <property>ja37/radio/fr31/s-r</property>
        <factor>60</factor>
        <offset-deg>-30</offset-deg>
        <center>
            <x-m>  0.02791 </x-m>
            <y-m>  0 </y-m>
            <z-m>  0.0034 </z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>        
        <increase>
            <binding>
                <command>property-adjust</command>
                <property>ja37/radio/fr31/s-r</property>
                <factor>1</factor>
                <step>1</step>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
           </binding>
        </increase>
        <decrease>
            <binding>
                <command>property-adjust</command>
                <property>ja37/radio/fr31/s-r</property>
                <factor>1</factor>
                <step>-1</step>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
            </binding>
        </decrease>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>switchMik</object-name>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>rdioMik</tooltip-id>
                <label>Microphone switch for comm. with mechanic or for tape recording</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>switchMik</object-name>
        <property>ja37/radio/fr31/s-m</property>
        <factor>60</factor>
        <offset-deg>-30</offset-deg>
        <center>
            <x-m>  0.00831 </x-m>
            <y-m>  0 </y-m>
            <z-m>  0.0034 </z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>        
        <increase>
            <binding>
                <command>property-adjust</command>
                <property>ja37/radio/fr31/s-m</property>
                <factor>1</factor>
                <step>1</step>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
           </binding>
        </increase>
        <decrease>
            <binding>
                <command>property-adjust</command>
                <property>ja37/radio/fr31/s-m</property>
                <factor>1</factor>
                <step>-1</step>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
            </binding>
        </decrease>
    </animation>


