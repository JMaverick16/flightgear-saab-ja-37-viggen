                <tooltip-id>rdioVol</tooltip-id>
                <label>Volume</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>knobVol</object-name>
        <property>ja37/radio/fr31/knob-v</property>
        <factor>90</factor>
        <offset-deg>-135</offset-deg>
        <center>
            <x-m>  -0.04043 </x-m>
            <y-m>  0.04445 </y-m>
            <z-m>  0 </z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>        
        <increase>
            <binding>
                <command>property-adjust</command>
                <property>ja37/radio/fr31/knob-v</property>
                <factor>1</factor>
                <step>0.05</step>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
           </binding>
        </increase>
        <decrease>
            <binding>
                <command>property-adjust</command>
                <property>ja37/radio/fr31/knob-v</property>
                <factor>1</factor>
                <step>-0.05</step>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
            </binding>
        </decrease>
    </animation>

