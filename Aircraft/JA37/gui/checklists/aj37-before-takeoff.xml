<?xml version="1.0"?>

<PropertyList>
    <title>Before Takeoff</title>
    <item>
        <name>Transponder</name>
        <value>ON</value>
        <condition>
            <equals>
                <property>/instrumentation/transponder/inputs/knob-mode</property>
                <value>5</value>
            </equals>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>/instrumentation/transponder/inputs/knob-mode</property>
            <value>5</value>
        </binding>
        <marker>
            <x-m>-3.4174</x-m>
            <y-m>0.2893</y-m>
            <z-m>-0.0099</z-m>
            <scale>1.0000</scale>
        </marker>
    </item>
    <item>
        <name>Flight instruments, main and backup</name>
        <value>CHECK</value>
    </item>
    <item>
        <name>Master caution</name>
        <value>CHECK OFF</value>
        <marker>
            <x-m>-4.0446</x-m>
            <y-m>-0.1450</y-m>
            <z-m>0.4600</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <equals>
                <property>fdm/jsbsim/systems/indicators/master-warning/warning</property>
                <value>0</value>
            </equals>
        </condition>
    </item>
    <item>
        <name>Landing lights</name>
        <value>ON</value>
        <condition>
            <equals>
                <property>controls/electric/lights-land-switch</property>
                <value>1</value>
            </equals>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>controls/electric/lights-land-switch</property>
            <value>1</value>
        </binding>
        <marker>
            <x-m>-3.3843</x-m>
            <y-m>0.3850</y-m>
            <z-m>-0.0132</z-m>
            <scale>1.1818</scale>
        </marker>
    </item>
</PropertyList>
