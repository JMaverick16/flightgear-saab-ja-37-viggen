<?xml version="1.0"?>

<PropertyList>
    <title>Start Engine</title>
    <item>
        <name>Hold engine starter (2 sec)</name>
        <value>ON</value>
        <marker>
            <x-m>-3.7322</x-m>
            <y-m>-0.4727</y-m>
            <z-m>0.1116</z-m>
            <scale>3.0000</scale>
        </marker>
        <condition>
            <equals>
                <property>fdm/jsbsim/systems/electrical/starter-relay-R1</property>
                <value>1</value>
            </equals>
        </condition>
        <binding>
            <command>nasal</command>
            <script>
              setprop("controls/engines/engine[0]/starter-cmd-hold", 1);
              setprop("controls/engines/engine[0]/starter-cmd", 1);
              ja37.click();
              var timer = maketimer(2.1, func(){
                  setprop("controls/engines/engine[0]/starter-cmd-hold", 0);
              });
              timer.singleShot = 1;
              timer.simulatedTime = 1;
              timer.start();
            </script>
        </binding>
    </item>
    <item>
        <name>Start sys indicator</name>
        <value>LIT</value>
        <condition>
            <property>fdm/jsbsim/systems/indicators/start-system</property>
        </condition>
        <marker>
            <x-m>-3.8650</x-m>
            <y-m>0.2650</y-m>
            <z-m>0.1200</z-m>
            <scale>1.0000</scale>
        </marker>
    </item>
    <item>
        <name>Fan RPM</name>
        <value>54-60%</value>
        <condition>
            <and>
                <greater-than>
                    <property>/engines/engine[0]/n2</property>
                    <value>54</value>
                </greater-than>
                <less-than>
                    <property>/engines/engine[0]/n2</property>
                    <value>60</value>
                </less-than>
            </and>
        </condition>
        <marker>
            <x-m>-3.9678</x-m>
            <y-m>-0.1900</y-m>
            <z-m>0.2471</z-m>
            <scale>3.0000</scale>
        </marker>
    </item>
    <item>
        <name>Engine pressure ratio</name>
        <value>AROUND 1.0</value>
        <condition>
            <less-than>
              <property>/engines/engine[0]/epr-gauge</property>
              <value>1.1</value>
            </less-than>
        </condition>
        <marker>
            <x-m>-3.9678</x-m>
            <y-m>-0.1331</y-m>
            <z-m>0.2471</z-m>
            <scale>3.0000</scale>
        </marker>
    </item>
    <item>
        <name>Exhaust gas temperature</name>
        <value>MAX 400 C</value>
        <condition>
            <less-than>
              <property>fdm/jsbsim/propulsion/engine/outlet-temperature-degc</property>
              <value>400</value>
            </less-than>
        </condition>
        <marker>
            <x-m>-3.8000</x-m>
            <y-m>0.2800</y-m>
            <z-m>0.1100</z-m>
            <scale>1.7000</scale>
        </marker>
    </item>
    <item>
        <name>Engine</name>
        <value>Ready</value>
        <condition>
            <equals>
                <property>/engines/engine[0]/running</property>
                <value>1</value>
            </equals>
        </condition>
    </item>
    <item>
        <name>Toggle external power (menu)</name>
        <value>OFF</value>
        <binding>
            <command>property-assign</command>
            <property>fdm/jsbsim/systems/electrical/external/enable-cmd</property>
            <value>0</value>
        </binding>
        <condition>
            <equals>
                <property>fdm/jsbsim/systems/electrical/external/enable-cmd</property>
                <value>0</value>
            </equals>
        </condition>
    </item>
    <item>
        <name>SPAK</name>
        <value>CHECK ON</value>
        <marker>
            <x-m>-4.0400</x-m>
            <y-m>-0.2420</y-m>
            <z-m>0.4550</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <equals>
                <property>/instrumentation/joystick-indicator</property>
                <value>1</value>
            </equals>
        </condition>
    </item>
</PropertyList>
