<?xml version="1.0"?>

<PropertyList>

    <name type="string" n="0">Engine running, ready for takeoff. Parking brakes on.</name>

    <overlay>
        <fdm>
            <jsbsim>
                <fcs>
                    <canopy>
                        <engage type="bool">false</engage>
                    </canopy>
                </fcs>
                <propulsion>
                    <engine>
                        <cutoff-commanded type="bool">false</cutoff-commanded>
                    </engine>
                </propulsion>
                <systems>
                    <external>
                        <switch type="bool">false</switch>
                        <enable-cmd type="bool">false</enable-cmd>
                    </external>
                    <electrical>
                        <battery-drain-norm type="double">1</battery-drain-norm>
                    </electrical>
                </systems>
            </jsbsim>
        </fdm>
        <controls>
            <oxygen type="bool">true</oxygen>
            <altimeter-radar type="bool">true</altimeter-radar>
            <ventilation>
                <airconditioning-enabled type="bool">true</airconditioning-enabled>
            </ventilation>
            <engines>
                <engine n="0">
                    <starter-cmd type="bool">true</starter-cmd>
                    <cutoff type="bool">false</cutoff>
                    <reverser-cmd type="bool">false</reverser-cmd>
                </engine>
            </engines>
            <electric>
                <engine n="0">
                    <generator type="bool">true</generator>
                </engine>
                <main type="bool">true</main>
                <battery type="bool">true</battery>
                <reserve type="bool">false</reserve>
                <lights-ext-flash type="bool">true</lights-ext-flash>
                <lights-ext-beacon type="bool">true</lights-ext-beacon>
                <lights-ext-nav type="bool">true</lights-ext-nav>
                <lights-land-switch type="bool">true</lights-land-switch>
                <main type="bool">true</main>
            </electric>
            <fuel>
                <auto type="bool">true</auto>
            </fuel>
        </controls>
        <engines>
            <engine n="0">
                <running type="bool">true</running>
            </engine>
        </engines>
        <ja37>
            <radar>
                <enabled type="bool">true</enabled>
            </radar>
            <systems>
                <state type="string">ready</state>
            </systems>
            <hud>
                <units-metric type="bool">false</units-metric>
            </hud>
            <avionics>
                <!--<master-warning-button type="bool">true</master-warning-button>-->
            </avionics>
        </ja37>
    </overlay>

</PropertyList>